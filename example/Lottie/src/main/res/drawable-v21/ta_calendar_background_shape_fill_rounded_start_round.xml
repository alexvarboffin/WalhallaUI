<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="2dp"
        android:left="4dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <solid android:color="?attr/ta_surfaceTint" />
            <corners
                android:bottomLeftRadius="51dp"
                android:topLeftRadius="51dp" />
        </shape>
    </item>
    <item
        android:bottom="2dp"

        android:top="2dp">
        <shape android:shape="rectangle">
            <solid android:color="?attr/ta_primary" />
            <corners
                android:bottomLeftRadius="51dp"
                android:bottomRightRadius="51dp"
                android:topLeftRadius="51dp"
                android:topRightRadius="51dp" />
        </shape>
    </item>
</layer-list>